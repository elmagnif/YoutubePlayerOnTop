        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YouTubePlayerOnTop"
        mc:Ignorable="d"
        Title="EnterURL" Height="122" Width="300">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="70,17,0,0" TextWrapping="Wrap" Text="Lien de la video" VerticalAlignment="Top" Width="145"/>
        <Button x:Name="button" Content="Charger la video" HorizontalAlignment="Left" Margin="70,55,0,0" VerticalAlignment="Top" Width="145" Click="button_Click"/>

    </Grid>
</Window>
